{
  "name": "Volume 10x",
  "author": "Gurigraphics",
  "version": "1.0.2",
  "description": "Amplify the video volume to 10x.",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },
  "manifest_version":2,
  "content_scripts": [{
    "js": ["app.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "permissions": [
        "activeTab"
    ]
}
 
